<?xml version="1.0" encoding="UTF-8"?>
<deliverable-definition
  id="pdf-01"
  ditaarch:DITAArchVersion="1.2"
  xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
  class="- topic/topic deliverable-definition/deliverable-definition "
  domains="(topic deliverable-definition) (topic options-d) ">
  <title class="- topic/title ">PDF Generated Using PDF2 Transform</title>
  <deliverable-specs class="- topic/body deliverable-definition/deliverable-specs ">
    <processor-spec
      class="- topic/section deliverable-definition/processor-spec ">
      <processor-id class="- topic/p deliverable-definition/processor-id ">DITA Open Toolkit</processor-id>
      <option
        class="+ topic/dlentry options-d/option ">
        <option-name
          class="+ topic/dt options-d/option-name ">transtype</option-name>
        <option-value
          class="+ topic/dd options-d/option-value ">pdf</option-value>
      </option>
      <option
        class="+ topic/dlentry options-d/option ">
        <option-name
          class="+ topic/dt options-d/option-name ">generate.copy.outer</option-name>
        <option-value
          class="+ topic/dd options-d/option-value ">3</option-value>
      </option>
    </processor-spec>
    <result-spec
      class="- topic/section deliverable-definition/processor-spec ">
      <result-mime-type class="- topic/p deliverable-definition/result-mime-type ">application/pdf</result-mime-type> 
    </result-spec>
  </deliverable-specs>
</deliverable-definition>
